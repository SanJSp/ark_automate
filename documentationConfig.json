{
  "opts": {
    "template": "../frontend/node_modules/better-docs",
    "destination": "./../documentationWebsite"
  },
  "tags": {
    "allowUnknownTags": ["category", "component"]
  },
  "plugins": ["node_modules/better-docs/category"],
  "source": {
    "include": [
      "./../frontend/src",
      "./../server/utils/ssotToRobotParsing",
      "./../server/socket/socketHelperFunctions",
      "./../server/utils/testing/testDatabaseHandler"
    ],
    "exclude": ["./../frontend/node_modules", "./../server/node_modules"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  }
}
