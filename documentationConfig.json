{
  "opts": {
    "template": "../frontend/node_modules/better-docs",
    "destination": "./../documentationWebsite"
  },
  "tags": {
    "allowUnknownTags": ["category", "component"]
  },
  "plugins": ["node_modules/better-docs/category"],
  "source": {
    "include": ["./../client/src", "./../server/services", "./../server/utils"],
    "exclude": [
      "./../client/node_modules",
      "./../server/node_modules",
      "./../server/api/controllers",
      "./../server/utils/openApiDocumentation"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  }
}
